{
  "name": "tls-monitor",
  "version": "1.0.0",
  "description": "Monitor SSL/TLS certificates for your domains.",
  "main": "build/main.js",
  "repository": "git@github.com:zrosenbauer/tls-monitor.git",
  "author": "@zrosenbauer",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "type:check": "tsc --noEmit",
